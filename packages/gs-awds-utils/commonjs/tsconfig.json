  "compilerOptions": {
    "composite": true,
    "module": "commonjs",
    "outDir": "../dist/commonjs",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "declaration": true,
