{
  "name": "gs-awds-utils",
  "version": "0.0.3",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "rm -fr dist/* && tsc -b",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffets": false,
  "exports": {
    ".": "./dist/index.js",
    "./content": {
      "require": "./dist/commonjs/tailwind-content.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "content": [
        "dist/commonjs/tailwind-content.d.cts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "content": [
          "dist/commonjs/tailwind-content.d.cts"
        ]
      }
    }
  },
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts}"
  ],
  "devDependencies": {
    "@types/node": "^20.14.9",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0"
  }
}
