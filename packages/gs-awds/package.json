{
  "name": "gs-awds",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build:swc -w",
    "build": "rm -rf dist && pnpm build:swc && pnpm types",
    "build:swc": "swc ./types.ts ./*.tsx ./globals.css ./tailwind.cjs -d ./dist --copy-files",
    "types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "pnpm build && node workspace-deps.cjs",
    "lint": "TIMING=1 eslint . --ext .js,.jsx,.ts,.tsx",
    "index": "node generate-components-index.cjs",
    "clean": "rm -rf ./dist && rm -rf .turbo"
  },
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    },
    "./globals.css": "./dist/globals.css",
    "./types": "./dist/types.d.ts"
  },
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "tailwind": [
        "dist/tailwind.d.cts"
      ],
      "types": [
        "dist/types.d.ts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "tailwind": [
          "dist/tailwind.d.cts"
        ],
        "types": [
          "dist/types.d.ts"
        ]
      }
    }
  },
  "peerDependencies": {
    "next": "*",
    "next-themes": "^0.3.0",
    "react": "^17.0.0 || ^18.0.0-0",
    "react-dom": "^17.0.0 || ^18.0.0-0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.1",
    "@swc/helpers": "^0.4.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.1.5",
    "gs-awds-utils": "workspace:*",
    "lucide-react": "^0.396.0",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.52.0",
    "react-resizable-panels": "^2.0.19",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.6.5",
    "@types/react": "^18.3.3",
    "chokidar": "^3.6.0",
    "eslint-config-next": "15.0.0-canary.44",
    "next": "^0.1.0",
    "next-themes": "^0.3.0",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4"
  }
}
